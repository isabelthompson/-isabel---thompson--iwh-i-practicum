doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 #{title}
        
        a(href="http://localhost:3000") Return to the homepage
        br

        form(action="/update-cobj", method="post")
            label(for="age") Superhero Age:
            input(type="text", id="age", name="age", required)

            label(for="name") Dinosaur Name:
            input(type="text", id="name", name="name", required)

            label(for="dinosourtype") Type:
            input(type="text", id="dinosourtype", name="dinosourtype", required)

            button(type="submit") Submit Dino Info 
